  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "nodemon": "nodemon",
    "create-out-folder": "[ -d out ] || mkdir out",
    "lint": "tslint -c tslint.json \"./src/*.ts\"",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test:watch": "mocha -r ts-node/register src/**/*.spec.ts --watch-extensions ts --recursive --watch",
    "dev": "concurrently --raw \"npm run build:watch\"  \"npm run test:watch\"",
    "exec": "concurrently --raw \"npm run create-out-folder\" \"npm run build\"  \"node ./dist/app.js https://wiprodigital.com/\""
  },
